@model IEnumerable<WebApplication1.Models.Models.ManagerModels>

@{
    ViewBag.Title = "AllManagers";
}

<h2>AllManagers</h2>

<p>
    @Html.ActionLink("Add new Info", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            
            @Html.ActionLink("Details", "Details", new { id=item.Id }) 
           
        </td>
    </tr>
}

</table>
